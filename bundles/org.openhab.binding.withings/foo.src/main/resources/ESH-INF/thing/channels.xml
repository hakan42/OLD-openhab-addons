		<description>Fat Mass...</description>
		<state readOnly="true"/>
	</channel-type>

	<channel-type id="body-fat-percentage">
		<item-type>Number</item-type>
		<label>Mass Percentage</label>
		<description>Mass Percentage...</description>
		<state readOnly="true"/>
	</channel-type>

	<channel-type id="measured-at">
		<item-type>DateTime</item-type>
		<label>Measurement Date</label>
		<description>Date...</description>
		<state readOnly="true"/>
	</channel-type>

